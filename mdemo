
from  sklearn.ensemble import RandomForestClassifier
import  pandas as pd
from sklearn.ensemble import RandomForestClassifier
from sklearn.metrics import accuracy_score
from sklearn.model_selection import  train_test_split


data=pd.read_csv('heart.csv')
x=data.drop('target', axis =1 )
y=data['target']
x_test,x_train, y_test,y_train =train_test_split(x,y ,train_size=0.2, random_state=42)
rf_model=RandomForestClassifier(n_estimators=100, random_state=43)
rf_model.fit(x_train,y_train)
y_pre=rf_model.predict(x_test)
acc=accuracy_score(y_pre,y_test)
print(acc)

